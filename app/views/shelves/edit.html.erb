<center><h2>Rename Shelf</center></h2>
<%= form_for(@shelf, html: {class: "form-horizontal"}) do |f| %>
	<%= render partial: 'form', locals: { f: f, shelf: @shelf } %>
	<div class="form-group">
	    <div class="col-sm-offset-4 col-sm-4">
	        <%= f.submit "Save Shelf", class: "btn btn-primary" %>
	</div>
<% end %>	